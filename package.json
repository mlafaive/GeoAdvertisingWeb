{
  "name": "react-webpack-babel-kit-2",
  "version": "1.0.0",
  "description": "React Webpack 2.0 babel kit with bootstrap framework",
  "main": "./public/index.html",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --hot --inline --history-api-fallback --content-base public/",
    "start:dev": "webpack-dev-server --history-api-fallback --config ./webpack.dev.config.js  --progress --colors",
    "build:dev": "webpack --config ./webpack.dev.config.js --progress --colors",
    "build": "webpack -p --config ./webpack.prod.config.js --progress --colors",
    "deploy": "webpack -p --config ./webpack.prod.config.js --progress --colors && firebase deploy"
  },
  "author": "mitch-lafaive",
  "license": "MIT",
  "devDependencies": {
    "axios": "^0.17.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "bootstrap-sass": "3.3.7",
    "css-hot-loader": "1.1.1",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.2",
    "firebase-tools": "^3.16.0",
    "html-to-draftjs": "^1.1.0",
    "node-sass": "^4.5.3",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-hot-loader": "3.0.0-beta.7",
    "react-scripts": "^1.0.17",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "0.5.8",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^4.0.0",
    "create-react-app": "^1.4.3",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "^3.3.1",
    "jump.js": "^1.0.2",
    "popper.js": "^1.12.9",
    "prop-types": "^15.6.0",
    "react-addons-css-transition-group": "^15.0.2",
    "react-bootstrap": "^0.31.5",
    "react-css-transition": "^0.7.4",
    "react-scripts": "^1.0.17"
  }
}
